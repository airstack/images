{
  "services": {
    "mysql": {
      "state": "up",
      "enable": "chpst -e /env/postgres -u root sh /package/airstack/mysql/enable",
      "disable": "",
      "run": "exec chpst -u mysql /usr/lib/postgresql/9.4/bin/postgres --config-file=/etc/postgresql/9.4/main/postgresql.conf",
      "check": "",
      "finish": "",
      "log": "exec chpst -u log logger -t postgres -p daemon.notice",
      "tags": ["postgres", "database", "db"],
      "input": [{"source": "any", "destination": "any", "port": "5432", "protocol": "tcp", "tags": [ "" ]}],
      "output": [{"source": "any", "destination": "any", "port": "5432", "protocol": "tcp","tags": [ "" ]}]
    }
  }
}
